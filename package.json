    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.2.0",
    "@types/chai": "^4.3.4",
    "@types/inquirer": "^8.2.5",
    "@types/inquirer-autocomplete-prompt": "^2.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.11",
    "@types/pretty-quick": "^3.1.1",
    "@types/yargs": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
