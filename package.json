    "inquirer-autocomplete-prompt": "^2.0.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.3",
    "pretty-quick": "^3.1.3",
    "sort-package-json": "^2.3.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tsup": "^6.5.0",
    "typechain": "^8.1.1",
    "typescript": "^4.9.4",
    "yargs": "^17.6.2"
